{
  "schemaVersion": 1,
  "id": "memento-mc",
  "version": "${version}",
  "name": "memento-mc",
  "description": "Automatic world renewal guided by simple player-defined memory anchors.",
  "authors": [],
  "contact": {},
  "license": "MIT",
  "icon": "assets/memento-mc/icon.png",
  "environment": "server",
  "entrypoints": {
    "main": [
      "ch.oliverlanz.mcmods.mementomc.mementoMc.MementoMc"
    ]
  },
  "mixins": [
    "memento-mc.mixins.json",
    {
      "config": "memento-mc.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric-language-kotlin": ">=${kotlin_loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
